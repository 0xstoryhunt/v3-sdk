{
  "name": "@storyhunt/v3-sdk",
  "version": "1.1.1",
  "description": "An SDK for building applications on top of Storyhunt V3",
  "repository": "https://github.com/0xstoryhunt/v3-sdk.git",
  "keywords": [
    "storyhunt",
    "story",
    "v3-sdk"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/v3-sdk.esm.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsdx build",
    "lint": "tsdx lint src",
    "release": "semantic-release",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/solidity": "^5.0.9",
    "@storyhunt/default-list": "^1.1.4",
    "@storyhunt/sdk-core": "^1.1.1",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "babel-jest": "25.5.1",
    "tsdx": "^0.14.1"
  },
  "resolutions": {
    "regenerator-runtime": "^0.14.1"
  },
  "resolutionsComments": {
    "regenerator-runtime": "Fixes https://github.com/facebook/regenerator/pull/480. It can be removed when `tsdx` updates their dependencies."
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": false
  },
  "release": {
    "extends": "semantic-release-monorepo",
    "branches": [
      {
        "name": "main",
        "prerelease": false
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": "../../publishing/release-rules.cjs"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/exec",
        {
          "successCmd": "git restore yarn.lock && yarn",
          "failCmd": "git restore yarn.lock && yarn",
          "execCwd": "../.."
        }
      ]
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
